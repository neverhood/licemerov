<h1>Hello, taht is main page</h1>

<%= render :partial => 'main/forms/parent' if current_user %>


  <table id='root-comments'>
    <% @entries.each do |entry| %>
        <%= render :partial => 'main/parent', :locals => {:root_entry => entry} %>
    <% end %>
  </table>

  <div class='more-entries button' id='main-show-more' data-page='1'>
    <%= t(:show_more) %>
  </div>



<%= render :partial => 'main/forms/response' %>
